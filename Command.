pwd          # показать, где ты находишься
ls           # список файлов и папок
ls -l        # список с правами и владельцами
cd           # переход по папкам
mkdir test   # создать папку
touch file1  # создать пустой файл
rm file1     # удалить файл
rmdir test   # удалить папку
man ls       # помощь по команде
